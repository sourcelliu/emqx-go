{
  "name": "emqx-go-dashboard-e2e-tests",
  "version": "1.0.0",
  "description": "Comprehensive E2E testing framework for EMQX-GO Dashboard with 500+ test cases",
  "main": "dashboard-e2e-runner.js",
  "scripts": {
    "test": "node dashboard-e2e-runner.js",
